{
    "name": "mango",
    "homepage": ".",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@aws-sdk/client-dynamodb": "^3.958.0",
        "@reown/appkit": "1.7.15",
        "@reown/appkit-adapter-wagmi": "1.7.15",
        "@tanstack/react-query": "^5.90.14",
        "@testing-library/jest-dom": "^5.17.0",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^13.5.0",
        "ajv": "^8.17.1",
        "ajv-keywords": "^5.1.0",
        "assert": "^2.0.0",
        "aws-sdk": "^2.1693.0",
        "axios": "^1.13.2",
        "bootstrap": "^5.3.8",
        "browserify-zlib": "latest",
        "concurrently": "^9.2.1",
        "cors": "^2.8.5",
        "crypto-browserify": "^3.12.1",
        "dotenv": "^16.6.1",
        "http": "npm:http-browserify",
        "http-browserify": "latest",
        "https": "npm:https-browserify",
        "https-browserify": "latest",
        "os-browserify": "^0.3.0",
        "path": "^0.12.7",
        "path-browserify": "^1.0.1",
        "process": "^0.11.10",
        "qs": "^6.13.0",
        "react": "^18.3.1",
        "react-app-rewired": "^2.2.1",
        "react-bootstrap": "^2.10.10",
        "react-bootstrap-icons": "^1.11.6",
        "react-dnd": "^14.0.5",
        "react-dnd-html5-backend": "^14.1.0",
        "react-dom": "^18.3.1",
        "react-fetch": "^0.0.9",
        "react-router-dom": "^7.11.0",
        "react-scripts": "^5.0.1",
        "react-select": "^5.10.2",
        "stream": "^0.0.2",
        "stream-browserify": "^3.0.0",
        "stream-http": "^3.2.0",
        "url": "latest",
        "v4": "^0.0.1",
        "viem": "^2.43.3",
        "vm-browserify": "^1.1.2",
        "wagmi": "^2.19.5",
        "zlib": "npm:browserify-zlib"
    },
    "presets": [
        "react-app"
    ],
    "plugins": [
        "@babel/plugin-transform-private-property-in-object"
    ],
    "scripts": {
        "start": "react-app-rewired start",
        "build": "NODE_OPTIONS='--no-deprecation' BROWSERSLIST_IGNORE_OLD_DATA=true ESLINT_NO_DEV_ERRORS=true DISABLE_ESLINT_PLUGIN=true react-app-rewired build",
        "build:analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
        "build:production": "GENERATE_SOURCEMAP=false react-app-rewired build",
        "test": "react-app-rewired test",
        "test:watch": "react-app-rewired test --watch",
        "test:coverage": "react-app-rewired test --coverage --watchAll=false",
        "test:ci": "CI=true react-app-rewired test --coverage --watchAll=false",
        "eject": "react-scripts eject",
        "postinstall": "npx --yes update-browserslist-db@latest > /dev/null 2>&1 || true",
        "verify:environment": "node scripts/verify-environment.js",
        "verify:contracts": "node scripts/verify-contracts.js",
        "verify:rpc": "node scripts/verify-rpc.js",
        "verify:explorers": "node scripts/verify-explorers.js",
        "verify:settings": "node scripts/verify-settings.js",
        "verify:features": "node scripts/verify-features.js",
        "verify:all": "node scripts/verify-all.js"
    },
    "overrides": {
        "typescript": "4.9.5",
        "glob": "^10.3.10",
        "rimraf": "^5.0.5",
        "sourcemap-codec": "npm:@jridgewell/sourcemap-codec@^1.4.15"
    },
    "engines": {
        "node": "24.x",
        "npm": ">=10.x"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@babel/plugin-transform-private-property-in-object": "^7.25.9",
        "@playwright/test": "^1.40.0",
        "browserslist": "^4.24.0",
        "identity-obj-proxy": "^3.0.0",
        "jest-axe": "^8.0.0",
        "jest-transform-stub": "^2.0.0",
        "string-replace-loader": "^3.1.0",
        "web-vitals": "^4.2.3"
    }
}
